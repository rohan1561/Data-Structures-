class Dequeue:
    def __init__(self):
        self.d = []

    def is_empty(self):
        return self.d == []

    def remove_front(self):
        return self.d.pop()

    def remove_rear(self):
        return self.d.pop(0)

    def add_front(self, item):
        self.d.append(item)

    def add_rear(self, item):
        self.d.insert(0, item)

    def size(self):
        return len(self.d)

